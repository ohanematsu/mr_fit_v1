<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mr_fit_v1.SettingsActivity"
    tools:ignore="MergeRootFrame" >

    <TableLayout
        android:background="#e0e0e0"
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_height="wrap_content" >

        <TableRow
            android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Account"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
             android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Exercise Time Goal"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
             android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Calorie Goal"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
             android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Alarm Time Interval"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
             android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Receive Reminder"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <CheckBox
                android:id="@+id/checkBox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </TableRow>

        <TableRow
             android:background="@android:color/white"
            android:layout_margin="@dimen/table_margin"
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:text="Avatar"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

    </TableLayout>

</FrameLayout>
